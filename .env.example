# OpenRouter API Configuration
OPENROUTER_API_KEY=your-api-key-here

# Optional: For OpenRouter rankings/analytics (leave blank if not needed)
OPENROUTER_SITE_URL=
OPENROUTER_SITE_NAME=

# Compression Models
OPENROUTER_MODEL=google/gemini-2.5-flash
OPENROUTER_MODEL_LARGE=anthropic/claude-opus-4.5

# Server Configuration
PORT=3000

# Compression Configuration
COMPRESSION_CONCURRENCY=10
COMPRESSION_TIMEOUT_MS=5000
COMPRESSION_RETRY_TIMEOUT_MS=10000
COMPRESSION_MAX_RETRIES=4
COMPRESSION_MIN_TOKENS=20
COMPRESSION_THINKING_THRESHOLD=1000
