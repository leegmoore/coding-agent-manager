{
  "name": "svp-cli",
  "version": "0.1.0",
  "description": "Session & Vibe Protocol - CLI toolkit for AI agents",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "svp": "dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "cli",
    "ai",
    "agent",
    "claude",
    "session"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
